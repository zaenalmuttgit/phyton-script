    "Every tag tells a story - let yours begin with (LUGGAGE TAG COLLECTIONS), then unfold more in (TENGAH) and (NGAREP) at (BLACKDOP)."
    ]



# Fungsi untuk menghasilkan HTML deskripsi
def generate_html():
    intro1 = random.choice(intro_paragraphs1)
    intro2 = random.choice(intro_paragraphs2)
    h3_intro = random.choice(intro_h3)
    care = random.choice(care_instructions)
    penutup = random.choice(kalimat_penutup)
    visit = random.choice(visit_more)


    details = "\n".join([
        random.choice(product_specs["satu"]),
        random.choice(product_specs["dua"]),
        random.choice(product_specs["tiga"]),
        random.choice(product_specs["empat"])
    ])



    return f"""
<h3><strong>{base_title}</strong></h3>
<h4><strong>{h3_intro}</strong></h4>
<p>{intro1}</p>
<p>{intro2}</p>
<h4><strong>Key Features:</strong></h4>
<ul>{details}</ul>
<h4><strong>Size Guide:</strong></h4>
<table style="border-collapse: collapse; width: 25.4023%;" border="1">
<tbody>
<tr>
<td style="width: 13.5632%;"></td>
<td style="width: 11.8391%;"><strong>One size</strong></td>
</tr>
<tr>
<td style="width: 13.5632%;"><strong>Width, in</strong></td>
<td style="width: 11.8391%;">3.00</td>
</tr>
<tr>
<td style="width: 13.5632%;"><strong>Height, in</strong></td>
<td style="width: 11.8391%;">4.50</td>
</tr>
<tr>
<td style="width: 13.5632%;"><strong>Depth, in</strong></td>
<td style="width: 11.8391%;">0.16</td>
</tr>
</tbody>
</table>
<h4><strong>Care Instructions:</strong></h4>
<ul>{care}</ul>
<p>{penutup}</p>
<p>{visit}</p>
"""

# Buat file Excel dan simpan
wb = openpyxl.Workbook()
ws = wb.active
ws.title = "Luggage Tag Descriptions"
ws.append(["Judul", "Deskripsi (HTML)"])

total = 2369  # Ubah jumlah deskripsi di sini jika perlu
seen = set()

while len(seen) < total:
    html = generate_html()
    if html not in seen:
        seen.add(html)
        ws.append([base_title, html])

wb.save("BLACKDOP_LUGGAGE_TAG.xlsx")
print("✅ File berhasil dibuat.")
